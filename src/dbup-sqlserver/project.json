{
    "version": "1.0.0-*",
    "title": "DbUp MS Sql Server Support",
    "name": "dbup4-sqlserver",
    "description": "**  Unofficial beta release of version 4.0 **\nDbUp makes it easy to deploy and upgrade SQL Server databases by running change scripts.",
    "authors": [ "Paul Stovell", "Jim Burger", "Jake Ginnivan", "Damian Maclennan" ],
    "packOptions": {
        "projectUrl": "http://dbup.github.com",
        "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
        "releaseNotes": "https://github.com/DbUp/DbUp/releases",
        "iconUrl": "https://raw.github.com/DbUp/DbUp/master/src/Information/dbup-icon.png"
    },
    "buildOptions": {
        "keyFile": "../dbup.snk"
    },
    "dependencies": {
        "dbup-core": { "target": "project" }
    },
    "frameworks": {
        "netstandard1.3": {
            "imports": "dnxcore50",
            "dependencies": {
                "NETStandard.Library": "1.6.0",
                "System.Data.SqlClient": "4.1.0"
            }
        },
        "net35": {
            "frameworkAssemblies": {
                "System.Data": "2.0.0.0"
            },
            "buildOptions": {
                "define": [
                    "SUPPORTS_SQL_CONTEXT"
                ]
            }
        }
    }
}
